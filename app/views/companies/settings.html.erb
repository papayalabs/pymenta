<%= javascript_include_tag 'document_types.jsx',
                    {
                        data: current_user.company.document_types,
                        page_header: t('.title', :default => DocumentType.model_name.human.pluralize.titleize),
                        form_data: {
                            account_types: DocumentType::ACCOUNT_TYPES,
                            stock_types: DocumentType::STOCK_TYPES,
                            select_prompt: t('.select', :default => t("helpers.links.select")),
                        }
                    } %>

